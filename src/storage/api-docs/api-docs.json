{
    "openapi": "3.0.0",
    "info": {
        "title": "ZooClick API",
        "description": "API documentation for Pet and Vaccination management",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8080",
            "description": "Local server"
        },
        {
            "url": "/api"
        }
