    "components": {
        "schemas": {
            "Pet": {
                "title": "Pet",
                "description": "Pet resource",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Lucky"
                    },
                    "type": {
                        "type": "string",
                        "example": "dog"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-09-19T12:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-09-19T12:00:00Z"
                    }
                },
                "type": "object"
            },
            "Vaccination": {
                "title": "Vaccination",
                "description": "Vaccination resource",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "pet_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "serial_number": {
                        "type": "string",
                        "example": "5YY9D-A8ff"
                    },
                    "country": {
                        "type": "string",
                        "example": "USA"
                    },
                    "vaccinated_at": {
                        "type": "string",
                        "format": "date",
                        "example": "2025-09-19"
                    },
                    "valid_days": {
                        "type": "integer",
                        "example": 365
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "http",
                "scheme": "bearer",
                "description": "Введите токен после авторизации. Пример: Bearer {token}"
            }
        }
